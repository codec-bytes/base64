{
  "name": "@aureooms/js-codec-base64",
  "description": "Base64 encoder/decoder for JavaScript",
  "version": "2.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-codec-base64/issues"
  },
  "dependencies": {
    "@aureooms/js-codec": "^3.0.0",
    "@aureooms/js-error": "^4.0.0",
    "@aureooms/js-itertools": "^4.0.0",
    "@aureooms/js-mapping": "^3.1.0"
  },
  "devDependencies": {
    "@aureooms/js-codec-ascii": "2.0.0",
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "ava": "3.8.2",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.3",
    "nyc": "15.0.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-codec-base64",
  "keywords": [
    "base64",
    "codec",
    "decode",
    "encode",
    "strings"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-codec-base64"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
